# LDAP配置示例
# 复制此文件为.env并根据实际情况修改

# 是否启用LDAP认证
LDAP_ENABLED=true

# LDAP服务器地址
LDAP_SERVER='ldap://XX.XX.XX.XX'

# LDAP服务器端口
LDAP_PORT=389

# LDAP绑定DN
LDAP_BIND_DN='cn=manager,dc=XXXX,dc=XXXX,dc=com'

# LDAP绑定密码
LDAP_BIND_PASSWORD='your_ldap_password'

# LDAP用户搜索基础DN
LDAP_USER_SEARCH_BASE='dc=XXXX,dc=XXXX,dc=com'

# LDAP用户搜索过滤器
LDAP_USER_SEARCH_FILTER='(!(pwdAccountLockedTime=000001010000Z))'
